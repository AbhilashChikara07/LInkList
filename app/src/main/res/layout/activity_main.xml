<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/mSingleLinkList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="Single Link List"
        android:textStyle="bold" />

    <Button
        android:id="@+id/mDoubleLinkList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="Double Link List"
        android:textStyle="bold" />

    <Button
        android:id="@+id/mSingleCircularList"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="Single Circular List"
        android:textStyle="bold" />

    <Button
        android:id="@+id/interViewQuestion"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="Question"
        android:textStyle="bold" />

    <Button
        android:id="@+id/stackImplementation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="Stack"
        android:textStyle="bold" />

    <Button
        android:id="@+id/mQuestionActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="5dp"
        android:text="Question"
        android:textStyle="bold" />

</LinearLayout>
