<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.chikara.linklist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".DeleteLastNode" />
        <activity android:name=".DeleteFirstNode" />
        <activity android:name=".LinkListInsertion" />
        <activity android:name=".InsertionAtBeginning" />
        <activity android:name=".InsertNodeAtPosition" />
        <activity android:name=".LinkListInsertNodeAtEnd" />
        <activity android:name=".LinkListTraversingElement" />

    </application>

</manifest>